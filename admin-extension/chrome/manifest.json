{
  "name": "AdminOps Extension",
  "version": "0.0.1",
  "description": "Extension for administration on GCP",
  "permissions": ["tabs", "identity"],
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_title": "AdminOps Extension"
  },
  "oauth2": {
    "scopes":["https://www.googleapis.com/auth/userinfo.email"]
  },
  "icons": { "16": "icon_16.png",
    "48": "icon_48.png",
   "128": "icon_128.png" },
  "manifest_version": 2
}
