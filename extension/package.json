{
  "name": "gcp-admin-extension",
  "version": "0.0.1",
  "description": "Extension for administration on GCP",
  "main": "index.js",
  "author": "Rishab Luthra",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "ng": "ng",
    "start": "npm run watch",
    "build:angular": "ng build",
    "watch:angular": "ng build --watch --delete-output-path false",
    "build:chrome": "webpack --config webpack.config.js --mode=development",
    "watch:chrome": "webpack --config webpack.config.js -w --mode=development",
    "build": "npm run clean && npm run build:angular && npm run build:chrome",
    "watch": "npm-run-all --parallel watch:*",
    "clean": "rimraf dist",
    "test:chrome": "jasmine-ts --config='chrome/jasmine.json'",
    "test": "npm-run-all test:*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "core-js": "^2.6.11",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "^9.1.0",
    "@types/chrome": "0.0.81",
    "@types/jasmine": "^3.5.11",
    "@types/node": "^12.12.32",
    "bestzip": "^2.1.5",
    "codelyzer": "^5.2.2",
    "copy-webpack-plugin": "^6.0.2",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "ts-loader": "^7.0.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-extension-reloader": "^1.1.4"
  }
}
