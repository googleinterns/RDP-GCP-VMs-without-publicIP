{
    "name": "Extension for administration on GCP",
    "version": "1.0",
    "description": "Extension for administration on GCP",
    "permissions": ["https://b.corp.google.com/*", "https://pantheon.corp.google.com/*", "activeTab", "declarativeContent", "storage", "tabs", "webNavigation", "webRequest"],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_title": "gCloud Admin Extension"
    },
    "content_scripts": [{
        "js": ["js/pantheon_content.js"],
        "run_at": "document_end",
        "matches": ["https://b.corp.google.com/*", "https://pantheon.corp.google.com/*"]
      }],
    "manifest_version": 2
}
